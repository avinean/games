    "create": "node src/@commands/create-template",
    "release": "npx webpack --config webpack/webpack.config.js -p --proj",
    "// ---------------------": "",
    "racing": "npm run build-res racing && npm run watch racing",
    "knife": "npm run build-res knife && npm run watch knife"
