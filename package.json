  "description": "npm install\r npm start",
  "scripts": {
    "start": "npx webpack --config webpack.config.js --watch",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run start\"",
    "server": "webpack-dev-server --mode development --open",
    "dev_new": "node ./webpack-server.js dev",
    "// --------------------": "",
    "build-res": "node src/@commands/build-resources",
    "watch": "npx webpack --config webpack/webpack.config.js --proj",
